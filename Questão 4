package questões;
	import java.util.Scanner;
	public class Q4 {
		public static void main(String[] args) {

			// Cria um Scanner para ler entradas do usuário
			
			Scanner scanner = new Scanner(System.in);

			// Lê uma string do usuário
			        System.out.print("Digite uma string: ");

			String input = scanner.nextLine();

			// Cria um segundo Scanner para ler os tokens da string
			Scanner tokenScanner = new Scanner(input);

			// Itera sobre cada token da string e verifica se começa com "s"
			while (tokenScanner.hasNext()) {
			            

			String token = tokenScanner.next();
			            
			           
			if (token.startsWith("s")) {
			                System.out.println(token);
			            }
			        }
     
			// Fecha os Scanners
			        scanner.close();
			        tokenScanner.close();
			    }
			}
